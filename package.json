{
  "name": "jsui-perf-test",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "::dev:bench:dbmon": "BABEL_ENV=development webpack-dev-server --config webpack/webpack.bench.dbmon.babel.js --mode development",
    "::dev:bench:triangle": "BABEL_ENV=development webpack-dev-server --config webpack/webpack.bench.triangle.babel.js --mode development",
    "::dev:renderer:remote": "BABEL_ENV=development webpack-dev-server --config webpack/webpack.renderer.remote.babel.js --mode development",
    "dev:bench:dbmon:react": "yarn ::dev:bench:dbmon --env.lib=react",
    "dev:bench:dbmon:preact": "yarn ::dev:bench:dbmon --env.lib=preact",
    "dev:bench:dbmon:jsui-polyfill": "yarn ::dev:bench:dbmon --env.lib=jsui --env.polyfill=force",
    "dev:bench:dbmon:jsui": "yarn ::dev:bench:dbmon --env.lib=jsui",
    "dev:bench:dbmon:jsui-iframe": "yarn ::dev:bench:dbmon --env.lib=jsui-iframe --env.polyfill=force",
    "dev:bench:dbmon:jsui-webrtc": "yarn ::dev:bench:dbmon --env.lib=jsui-webrtc --env.polyfill=force --env.serializer=json",
    "dev:bench:triangle:react": "yarn ::dev:bench:triangle --env.lib=react-experimental",
    "dev:renderer:iframe": "yarn ::dev:renderer:remote --env.polyfill=force --env.pipe=postmessage",
    "dev:renderer:webrtc": "yarn ::dev:renderer:remote --env.polyfill=force --env.pipe=webrtc --env.serializer=json",
    "::start:bench:dbmon": "BABEL_ENV=production webpack-dev-server --config webpack/webpack.bench.dbmon.babel.js --mode production",
    "::start:bench:triangle": "BABEL_ENV=production webpack-dev-server --config webpack/webpack.bench.triangle.babel.js --mode production",
    "::start:renderer:remote": "BABEL_ENV=production webpack-dev-server --config webpack/webpack.renderer.remote.babel.js --mode production",
    "start:bench:dbmon:react": "yarn ::start:bench:dbmon --env.lib=react",
    "start:bench:dbmon:preact": "yarn ::start:bench:dbmon --env.lib=preact",
    "start:bench:dbmon:jsui-polyfill": "yarn ::start:bench:dbmon --env.lib=jsui --env.polyfill=force",
    "start:bench:dbmon:jsui": "yarn ::start:bench:dbmon --env.lib=jsui",
    "start:bench:dbmon:jsui-iframe": "yarn ::start:bench:dbmon --env.lib=jsui-iframe --env.polyfill=force",
    "start:bench:dbmon:jsui-webrtc": "yarn ::start:bench:dbmon --env.lib=jsui-webrtc --env.polyfill=force --env.serializer=json",
    "start:bench:triangle:react": "yarn ::start:bench:triangle --env.lib=react-experimental",
    "start:renderer:iframe": "yarn ::start:renderer:remote --env.polyfill=force --env.pipe=postmessage",
    "start:renderer:webrtc": "yarn ::start:renderer:remote --env.polyfill=force --env.pipe=webrtc --env.serializer=json",
    "serve:webrtc-stun": "peerjs --port 9000",
    "hosts:list": "hostile list",
    "hosts:clean": "hostile remove jsui-server.local",
    "hosts:setup": "hostile set localhost jsui-server.local"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-unassert": "^3.0.1",
    "css-loader": "^3.5.1",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "exports-loader": "^0.7.0",
    "hostile": "^1.3.2",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.0.4",
    "prettier": "^2.0.4",
    "style-loader": "^1.1.3",
    "to-string-loader": "^1.1.6",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "assert": "^2.0.0",
    "classnames": "^2.2.6",
    "events": "^3.1.0",
    "lodash": "^4.17.19",
    "peer": "^0.5.2",
    "peerjs": "^1.2.0",
    "preact": "^10.3.4",
    "react": "^0.0.0-experimental-d7382b6c4",
    "react-15": "^0.2.0",
    "react-dom": "^0.0.0-experimental-d7382b6c4",
    "react-dom-core": "^0.1.1",
    "react-html-attributes": "^1.4.6",
    "stats.js": "^0.17.0"
  }
}
